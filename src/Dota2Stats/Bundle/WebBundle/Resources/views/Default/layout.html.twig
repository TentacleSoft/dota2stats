<!DOCTYPE html>
<html>
    <head>
        <title>{% block title %}Dota 2 Stats{% endblock %}</title>
        {% block stylesheets %}
            <link href="{{ asset('/../bundles/dota2statsweb/css/default.css') }}" type="text/css" rel="stylesheet" />
        {% endblock %}
        {% block javascripts %}
	        <script src="{{ asset('/../bundles/dota2statsweb/js/jquery-1.7.2.js') }}" type="text/javascript"></script>
	        <script src="{{ asset('/../bundles/dota2statsweb/js/underscore.js') }}" type="text/javascript"></script>
	        <script src="{{ asset('/../bundles/dota2statsweb/js/init.js') }}" type="text/javascript"></script>
	    {% endblock %}
    </head>
    <body>
        <div id="content">
            <div id="header">
                {% include 'Dota2StatsWebBundle:Default:header.html.twig' %}
            </div>
            <div id="body">
                {% block body %}{% endblock %}
            </div>
            <div id="footer">
                
            </div>
        </div>
    </body>
</html>